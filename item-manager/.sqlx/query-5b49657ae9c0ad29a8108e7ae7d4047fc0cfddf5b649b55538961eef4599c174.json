  "query": "\n                SELECT\n                rc.checkout_id,\n                rc.book_id,\n                rc.user_id,\n                rc.checked_out_at,\n                rc.returned_at,\n                b.title,\n                b.author,\n                b.isbn\n                FROM returned_checkouts AS rc\n                INNER JOIN books AS b USING(book_id)\n                WHERE rc.book_id = $1\n                ORDER BY rc.checked_out_at DESC\n            ",
