  "query": "\n                SELECT\n                c.checkout_id,\n                c.book_id,\n                c.user_id,\n                c.checked_out_at,\n                b.title,\n                b.author,\n                b.isbn\n                FROM checkouts AS c\n                INNER JOIN books AS b USING(book_id)\n                WHERE c.book_id = $1\n            ",
